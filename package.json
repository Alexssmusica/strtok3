  "bugs": {
    "url": "https://github.com/Borewit/strtok3/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/es6-promise": "0.0.32",
    "@types/fs-extra": "^3.0.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.37",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "debug": "^2.6.8",
    "gulp": "^3.9.1",
    "gulp-istanbul": "^1.1.2",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
